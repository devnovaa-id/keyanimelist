{
  "version": 2,
  "name": "nonim-anime-streaming",

  "routes": [
    {
      "handle": "filesystem"
    },

    {
      "src": "^/service-worker\\.js$",
      "dest": "/sw.js"
    },

    {
      "src": "^/api/otakudesu/v2/(.*)",
      "dest": "https://api.devnova.icu/api/otakudesu/v2/$1"
    },

    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],

  "headers": [
    {
      "source": "/(css|js|asset)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],

  "regions": ["sin1"]
}